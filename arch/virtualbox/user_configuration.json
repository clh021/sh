    "multilib"
  ],
  "archinstall-language": "English",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "Systemd-boot",
  "config_version": "2.6.3",
  "debug": false,
  "disk_encryption": null,
  "hostname": "archbox",
  "kernels": [
    "linux-zen"
  ],
  "locale_config": {
    "kb_layout": "us",
    "sys_enc": "UTF-8",
    "sys_lang": "en_US"
  },
  "mirror_config": {
    "mirror_regions": {
      "China": [
