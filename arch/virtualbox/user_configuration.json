  "network_config": {
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages": [
    "man-db",
    "man-pages-zh_cn",
    "zip",
    "ripgrep",
    "axel",
    "htop",
    "wget",
    "tk",
    "p7zip",
    "ark",
    "lrzip",
    "zsh",
    "unarchiver",
    "git",
    "openssh",
    "tree",
    "docker-compose",
    "neovim",
    "xclip",
    "fzf",
