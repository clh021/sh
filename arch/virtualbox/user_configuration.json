    "translate-shell",
    "wqy-microhei",
    "wqy-microhei-lite",
    "fcitx5-im",
    "fcitx5-chinese-addons",
    "bluez",
    "bluez-utils",
    "bluedevil"
  ],
  "parallel downloads": 0,
  "profile_config": null,
  "script": "guided",
  "silent": false,
  "swap": true,
  "timezone": "Asia/Shanghai",
  "version": "2.6.3",
  "custom-commands": [
    "systemctl enable sshd",
    "systemctl enable sddm",
    "systemctl enable docker",
    "systemctl enable bluetooth",
    "usermod -aG docker lee",
    "chsh -s /bin/zsh lee",
    "echo -e '[Wallet]\nEnabled=false' >> /home/lee/.config/kwalletrc",
    "echo -e 'GTK_IM_MODULE=fcitx\nQT_IM_MODULE=fcitx\nXMODIFIERS=@im=fcitx\nSDL_IM_MODULE=fcitx\nGLFW_IM_MODULE=ibus' >> /etc/environment",
  ]
}
