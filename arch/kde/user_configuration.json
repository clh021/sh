{
  "additional-repositories": ["multilib"],
  "archinstall-language": "English",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "Systemd-boot",
  "config_version": "2.6.3",
  "debug": false,
  "disk_encryption": null,
  "hostname": "archbox",
  "kernels": ["linux-zen"],
  "locale_config": {
    "kb_layout": "us",
    "sys_enc": "UTF-8",
    "sys_lang": "zh_CN"
  },
  "mirror_config": {
    "mirror_regions": {}
  },
  "network_config": {
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages": ["timeshift"],
  "parallel downloads": 0,
  "profile_config": {
    "profile": "desktop",
    "desktop": "plasma"
  },
  "script": "guided",
  "silent": false,
  "swap": true,
  "timezone": "Asia/Shanghai",
  "version": "2.6.3",
  "custom-commands": ["systemctl enable sddm"]
}
